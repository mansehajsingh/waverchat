<?xml version="1.0"?>
<authentication>
    <v1>
        <authenticatedEndpoints>
            <endpoint>
                <method>DELETE</method>
                <uri>api/v1/sessions</uri>
            </endpoint>
            <endpoint>
                <method>PUT</method>
                <uri>api/v1/users/{}</uri>
            </endpoint>
            <endpoint>
                <method>POST</method>
                <uri>api/v1/organizations</uri>
            </endpoint>
            <endpoint>
                <method>GET</method>
                <uri>api/v1/organizations/{}/members</uri>
            </endpoint>
        </authenticatedEndpoints>
    </v1>
</authentication>